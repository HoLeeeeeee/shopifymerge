  <script>
    $(function(){
      $(".common-faqs-navitem").click(function(){
        $(this).addClass("active").siblings().removeClass("active");
        let nindex = $(this).data("index");
        $(".common-faqs-britem").hide();
        $(".common-faqs-britem-"+nindex).fadeIn();
      });
      $(".common-faqs-britqatitle").click(function(){
        if($(this).next(".common-faqs-britqacon").is(":hidden")){
          $(this).addClass("open");
        }else{
           $(this).removeClass("open");
        }
        $(this).next(".common-faqs-britqacon").slideToggle();
      });
    })
  </script>
  
  
