             assign type2 = false
             assign type3 = false
             assign type4 = false
             assign type5 = false
             assign type6 = false
            assign type7 = false
             assign type8 = false
            assign type9 = false
             for block in section.blocks
              if block.settings.faqtype == 'type1'
               assign type1 = true
              elsif block.settings.faqtype == 'type2'
               assign type2 = true
               elsif block.settings.faqtype == 'type3'
               assign type3 = true
                elsif block.settings.faqtype == 'type4'
               assign type4 = true
             elsif block.settings.faqtype == 'type5'
               assign type5 = true
             elsif block.settings.faqtype == 'type6'
               assign type6 = true
             elsif block.settings.faqtype == 'type7'
