            letter-spacing: {{ block.settings.header_letter_spacing | append: 'px' }};
            color: {{ block.settings.header_color }};      
          }
          #text-with-icons-{{ block.id }} .text-with-icons__block-text p{
            font-size: {{ block.settings.content_size | append: 'px' }} ;
            font-weight: {{ block.settings.content_font_weight }};
            line-height: {{ block.settings.content_line_height | append: 'px'  }};
            letter-spacing: {{ block.settings.content_letter_spacing | append: 'px' }};
            color: {{ block.settings.content_color }};      
          }
          @media(max-width:767px){
            #text-with-icons-{{ block.id }} .text-with-icons__block-title h3{
              font-size: {{ block.settings.header_size_mo | append: 'px' }} ;
              font-weight: {{ block.settings.header_font_weight_mo }};
              line-height: {{ block.settings.header_line_height_mo | append: 'px'  }};
              letter-spacing: {{ block.settings.header_letter_spacing_mo | append: 'px' }}; 
            }
            #text-with-icons-{{ block.id }} .text-with-icons__block-text p{
              font-size: {{ block.settings.content_size | append: 'px' }} ;
              font-weight: {{ block.settings.content_font_weight }};
              line-height: {{ block.settings.content_line_height | append: 'px'  }};
