<style>
  .text-with-icons{
    margin-bottom: -60px;
  }
  #text-icons-{{ section.id }} .text-with-icons__blocks{
    flex-wrap: nowrap;
    flex-direction: row;
    justify-content: flex-start;
  }

</style>
{%- if section.settings.divider -%}<div class="section--divider">{%- endif -%}

<div
  class="text-with-icons"
  id="text-icons-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="text-with-icons"
  style="background-color: #f6f6f6; padding: 40px 0 0 0;"
>
  <div class="page-width swiper new-Swiper">
    {%- if section.settings.title != blank -%}
      <div
        class="
          text-with-icons__title
          section-header"
      >
        <h2 class="text-{{ section.settings.align_text }}">{{ section.settings.title }}</h2>
      </div>
    {%- endif -%}
    <div
      class="
        text-with-icons__blocks
        has-{{ section.settings.desktop_columns_per_row }}-per-row swiper-wrapper"
      data-block-count="{{ section.blocks.size }}"
      >
      {% for block in section.blocks %}
        <div id="text-with-icons-{{ block.id }}" class="swiper-slide text-with-icons__block text-{{ section.settings.align_text }}" {{ block.shopify_attributes }}>
          <div class="text-with-icons__block-icon">
            {% if block.settings.html_svg != blank %}
                  {{ block.settings.html_svg }}
