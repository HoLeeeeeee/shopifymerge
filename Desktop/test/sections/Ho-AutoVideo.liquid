
<div class="paautovideo-outbox">
  {% if section.settings.title != blank %}
  <div class="paautovideo-title">{{ section.settings.title }}</div>
   {% endif %}
  
  <div class="paautovideo-midtextbox">
   {% for block in section.blocks %}
  <div class="paautovideo-midtext-item">
        {% if block.settings.title != blank %}
  <div class="paautovideo-midtext-title">{{ block.settings.title }}</div>
   {% endif %}
   </div>
   {% endfor %}
  </div>
  
  
<div class="paautovideo-container">
  {% if section.settings.video_url != blank %}
   <video class="pa-sutovideo" width="100%" loop="loop" autoplay="autoplay"  webkit-playsinline="true" playsinline="true" muted="muted" >
    <source src="{{  section.settings.video_url }}">
  </video>

   {% endif %}
    
  </div>
</div>





{% schema %}
  {
    "name": "Parkinson autoVideo",
    "settings": [
         {
            "type": "html",
            "id": "title",
            "label": "title"
          },{
            "type": "text",
            "id": "video_url",
            "label": "video_url"
          }
    ],
    "blocks": [
      {
        "type": "block",
        "name": "block",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "title"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Parkinson autoVideo",
        "category": "Ho-Advance"
 
      }
    ]
  }
{% endschema %}

