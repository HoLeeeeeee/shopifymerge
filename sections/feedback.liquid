<div id= "feedback-page">
   <!--page header-->
{% if section.settings.title != blank %}
<div class="section-header text-center">
  <h1>{{ section.settings.title | escape }}</h1>
    <img style="width:10px" class="icoico2" src="{{'ys.png'|asset_img_url}}"  alt="" />
    <img style="width:10px" class="icoico2" src="{{'ys.png'|asset_img_url}}"  alt="" />
    <img style="width:10px" class="icoico2" src="{{'ys.png'|asset_img_url}}"  alt="" />
  <p>{{ section.settings.small_title | escape }}</p>
</div>
{% endif %} 
  
    <!--page body -->
  
  <div class="  grid {% unless section.blocks.size == 3 %}grid--uniform{% endunless %} grid--flush-bottom">
    {%- assign grid_item_width = 'medium-up--one-third' -%}
    {%- assign max_height = 345 -%}
    {% for block in section.blocks %}
      {%- assign column_width = grid_item_width -%}
            <div class="addmouseon grid__item {{ column_width }} text-{{ section.settings.align_text }}" {{ block.shopify_attributes }}>
                  {% if block.settings.image != blank %}
      {%- assign img_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
      {% include 'image-style' with image: block.settings.image, width: max_height, height: max_height, small_style: true, wrapper_id: wrapper_id, img_id: img_id %}
      <div id="{{ wrapper_id }}" class="  feature-row__image-wrapper js">
        <div style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;">
          <a target="_blank" href="{{ block.settings.button_link }}">
            <img class="feature-row__image lazyload"
                 src="{{ block.settings.image | img_url: '300x300' }}"
                 data-src="{{ img_url }}"
                 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                 data-aspectratio="{{ block.settings.image.aspect_ratio }}"
                 data-sizes="auto"
                 alt="{{ block.settings.image.alt | escape }}">
          </a>
        </div>
      </div>
      <noscript>
        <p>
          {{ block.settings.image | img_url: '480x480', scale: 2 | img_tag: block.settings.image.alt, 'feature-row__image' }}
        </p>
      </noscript>
      {% else %}
      <p>
        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
      </p>
      {% endif %}


      <div class="feedback-box">
        {% if block.settings.title != blank %}
        <h3 class="h4" title="{{ block.settings.title | escape }}">{{ block.settings.title | escape }}</h3>
       
        {% endif %}
        
        {% if block.settings.text != blank %}
        <div class="words">{{ block.settings.text }}</div>
        {% endif %}

        <div class="feed-info-logo">
          <div class="customer-info">
            {% if block.settings.small_image != blank %}

            {%- assign img_url = block.settings.small_image | img_url: '49x49' | replace: '_1x1.', '_{width}x.' -%}
            {% include 'image-style' with small_image: block.settings.small_image, width: max_width, height: max_height, small_style: true, wrapper_id: wrapper_id, img_id: img_id %}
            <div id="{{ wrapper_id }}" class="feature-row__small_image-wrapper js">
              <div style="padding-top:{{ 1 | divided_by: block.settings.ismall_image.aspect_ratio | times: 100}}%;">
                <img id="{{ img_id }}"
                     class="feature-row__small_image lazyload"
                     src="{{ block.settings.small_image | img_url: '300x300' }}"
                     data-src="{{ img_url }}"
                     data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                     data-aspectratio="{{ block.settings.small_image.aspect_ratio }}"
                     data-sizes="auto"
                     alt="{{ block.settings.small_image.alt | escape }}">
                
           {% if block.settings.name_text != blank %}
          <span class="name-info"> {{ block.settings.name_text }}</span>
          {% endif %}

          {% if block.settings.date_text != blank %}
          <span class="date-info"> {{ block.settings.date_text }}</span>
          {% endif %}
              </div>
            </div>
            {% else %}
            <img src="https://cdn.shopify.com/s/files/1/0033/1828/4343/files/feed-defaut-customer.png?8259085006067166196">
            {% endif %}
          </div>

        
           <div class="clear"></div>
        </div>
      </div>
              
     </div>
    
    
    
    
    
    
    {% endfor %}
    
  </div>

</div>

<div id="insta-feed"></div>

